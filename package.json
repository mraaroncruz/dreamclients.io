    "jsdom": "^15.1.1",
    "luxon": "^1.21.3",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.2.5",
    "mini-css-extract-plugin": "^1.3.3",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^4.1.0",
    "pre-commit": "^1.2.2",
    "pre-push": "^0.1.1",
    "rimraf": "^3.0.2",
    "rollup-plugin-terser": "^6.1.0",
